   ],
   "source": [
    "len(counter.keys())"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 23,
   "metadata": {},
   "outputs": [],
   "source": [
    "token_to_ind = {t:i for i, t in enumerate(bow_vocabulary)}\n",
    "def text_to_bow(text):\n",
    "    \"\"\" convert text string to an array of token counts. Use bow_vocabulary. \"\"\"\n",
    "    cnt = Counter()\n",
    "    cnt.update(text.split())\n",
    "    bow = np.zeros_like(bow_vocabulary, 'float32')\n",
    "    for token, num in cnt.items():\n",
    "        if token in token_to_ind:\n",
    "            bow[token_to_ind[token]] = num\n",
    "    return bow"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 24,
   "metadata": {},
   "outputs": [],
   "source": [
    "X_train_bow = np.stack(list(map(text_to_bow, texts_train)))\n",
    "X_test_bow = np.stack(list(map(text_to_bow, texts_test)))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 25,
   "metadata": {},
